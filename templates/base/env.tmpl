SERVER_HOST=0.0.0.0:8080
LOGLEVEL=debug
{{- if .WantDatabase}}
DSN={{.DSN}}
{{- if eq .WhatTypeOfDatabase "MySQL"}}
MYSQL_DATABASE="{{.DbName}}"
MYSQL_ROOT_PASSWORD="password"{{end}}
{{- if eq .WhatTypeOfDatabase "Postgres"}}
POSTGRES_DB="{{.DbName}}"
POSTGRES_PASSWORD="password"{{end}}
{{- end}}
GITHUB_TOKEN={{.GithubToken}}

